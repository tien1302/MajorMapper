<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebClient</title>

    <!-- favicon
      ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="~/img/favicon.ico" asp-append-version="true">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/font-awesome.min.css" asp-append-version="true">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/owl.carousel.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/owl.theme.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/owl.transitions.css" asp-append-version="true">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/normalize.css" asp-append-version="true">
    <!-- meanmenu icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/meanmenu.min.css" asp-append-version="true">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true">
    <!-- educate icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/educate-custon-icon.css" asp-append-version="true">
    <!-- morrisjs CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/morrisjs/morris.css" asp-append-version="true">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/scrollbar/jquery.mCustomScrollbar.min.css" asp-append-version="true">
    <!-- metisMenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/metisMenu/metisMenu.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/metisMenu/metisMenu-vertical.css" asp-append-version="true">
   
    <!-- x-editor CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/editor/select2.css">
    <link rel="stylesheet" href="~/css/editor/datetimepicker.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/editor/bootstrap-editable.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/editor/x-editor-style.css" asp-append-version="true">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/data-table/bootstrap-table.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/data-table/bootstrap-editable.css" asp-append-version="true">
    <!-- forms CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/form/all-type-forms.css" asp-append-version="true">
    <!-- modals CSS
       ============================================ -->
    <link rel="stylesheet" href="~/css/modals.css" asp-append-version="true">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/style.css" asp-append-version="true">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true">
    <!-- summernote CSS
        ============================================ -->
    <link rel="stylesheet" href="~/css/summernote/summernote.css" asp-append-version="true">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/js/vendor/modernizr-2.8.3.min.js" asp-append-version="true"></script>
    

</head>
<body>
    <!-- Start Left menu area -->
    <div class="left-sidebar-pro">
        <nav id="sidebar" class="">
            <div class="sidebar-header">
                <a href="index.html"><img class="main-logo" src="~/img/logo/logo.png" alt="" asp-append-version="true" /></a>
                <strong><a href="index.html"><img src="~/img/logo/logosn.png" alt="" asp-append-version="true" /></a></strong>
            </div>
            <div class="left-custom-menu-adp-wrap comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro">
                    <ul class="metismenu" id="menu1">
                        @if (@Context.Session.GetString("Role") == "Admin")
                        {
                            <li>
                                <a title="Tài khoản" href="/Account/Index" aria-expanded="false"><span class="educate-icon educate-professor icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Tài khoản</span></a>
                            </li>
                            <li>
                                <a title="Ngành nghề" href="/Major/Index" aria-expanded="false"><span class="educate-icon educate-course icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Ngành nghề</span></a>
                            </li>
                            <li>
                                <a title="Câu hỏi" href="/Question/Index" aria-expanded="false"><span class="educate-icon educate-library icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Câu hỏi</span></a>
                            </li>
                           
                        }
                        else
                        {
                            <li>
                                <a title="Slot" href="/Slot/Index" aria-expanded="false"><span class="educate-icon educate-library icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Slot</span></a>
                            </li>
                            <li>
                                <a title="Ngành nghề" href="/Major/Index" aria-expanded="false"><span class="educate-icon educate-course icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Ngành nghề</span></a>
                            </li>
                            <li>
                                <a title="Câu hỏi" href="/Question/Index" aria-expanded="false"><span class="educate-icon educate-library icon-wrap" aria-hidden="true"></span> <span class="mini-click-non">Câu hỏi</span></a>
                            </li>
                          
                        }

                    </ul>
                </nav>
            </div>
        </nav>
    </div>
    <!-- End Left menu area -->
    <div class="all-content-wrapper">
        <!-- Logo mobile -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="~/img/logo/logo.png" alt="" asp-append-version="true" /></a>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header -->
        <div class="header-advance-area">
            <div class="header-top-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="header-top-wraper">
                                <div class="row">
                                    <div class="col-lg-1 col-md-0 col-sm-1 col-xs-12">
                                        <div class="menu-switcher-pro" style="padding: 15px 0px;">
                                            <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                                                <i class="educate-icon educate-nav"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-7 col-sm-6 col-xs-12">
                                        <div class="header-top-menu tabl-d-n" style="padding: 15px 0px;">
                                            <ul class="nav navbar-nav mai-top-nav">
                                                <li class="nav-item">
                                                    <a href="#" class="nav-link">Trang chủ</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a href="#" class="nav-link">Hỗ trợ</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a href="#" class="nav-link">Chính sách</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                        <div class="header-right-info">
                                            <ul class="nav navbar-nav mai-top-nav header-right-menu">
                                               
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle"><i class="educate-icon educate-bell" aria-hidden="true"></i><span class="indicator-nt"></span></a>
                                                    <div role="menu" class="notification-author dropdown-menu animated zoomIn">
                                                        <div class="notification-single-top">
                                                            <h1>Notifications</h1>
                                                        </div>
                                                        <ul class="notification-menu">
                                                            <li>
                                                                <a href="#">
                                                                    <div class="notification-icon">
                                                                        <i class="educate-icon educate-checked edu-checked-pro admin-check-pro" aria-hidden="true"></i>
                                                                    </div>
                                                                    <div class="notification-content">
                                                                        <span class="notification-date">16 Sept</span>
                                                                        <h2>Advanda Cro</h2>
                                                                        <p>Please done this project as soon possible.</p>
                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                        <div class="notification-view">
                                                            <a href="#">View All Notification</a>
                                                        </div>
                                                    </div>
                                                </li>
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                                        
                                                        <span class="admin-name" style="min-width: 140px;text-align: right;">@Context.Session.GetString("Name")</span>
                                                        <i class="fa fa-angle-down edu-icon edu-down-arrow"></i>
                                                    </a>
                                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated zoomIn">
                                                     
                                                        <li>
                                                            <a href="/Account/Details/@Context.Session.GetInt32("AccountId")"><span class="edu-icon edu-user-rounded author-log-ic"></span>Thông tin tài khoản</a>
                                                        </li>
                                                        <li>
                                                            <a href="#"><span class="edu-icon edu-money author-log-ic"></span>Đổi mật khẩu</a>
                                                        </li>
                                                      
                                                        <li>
                                                            <a asp-action="Logout" asp-controller="Home"><span class="edu-icon edu-locked author-log-ic"></span>Đăng xuất</a>
                                                        </li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu start -->
            <div class="mobile-menu-area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="mobile-menu">
                                <nav id="dropdown">
                                    <ul class="mobile-menu-nav">
                                        <li><a href="/Account/Index">Tài khoản</a></li>
                                        <li><a href="/Major/Index">Ngành nghề</a></li>
                                        <li><a href="/Question/Index">Câu hỏi</a></li>
                                        <li><a href="/Slot/Index">Slot</a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu end -->
        </div>
        <!-- Main -->
        <main role="main" style="margin-top:20px;">
            @RenderBody()
        </main>

    </div>

    <!-- Static Table End -->
    <div class="footer-copyright-area">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>© 2023 - Đồ án tốt nghiệp Major Mapper</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- jquery
        ============================================ -->
    <script src="~/js/vendor/jquery-1.12.4.min.js" asp-append-version="true"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
    <!-- wow JS
        ============================================ -->
    <script src="~/js/wow.min.js" asp-append-version="true"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/js/jquery-price-slider.js" asp-append-version="true"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/js/jquery.meanmenu.js" asp-append-version="true"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
    <!-- sticky JS
        ============================================ -->
    <script src="~/js/jquery.sticky.js" asp-append-version="true"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/js/jquery.scrollUp.min.js" asp-append-version="true"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/js/scrollbar/jquery.mCustomScrollbar.concat.min.js" asp-append-version="true"></script>
    <script src="~/js/scrollbar/mCustomScrollbar-active.js" asp-append-version="true"></script>
    <!-- metisMenu JS
        ============================================ -->
    <script src="~/js/metisMenu/metisMenu.min.js" asp-append-version="true"></script>
    <script src="~/js/metisMenu/metisMenu-active.js" asp-append-version="true"></script>
    <!-- data table JS
        ============================================ -->
    <script src="~/js/data-table/bootstrap-table.js" asp-append-version="true"></script>
    <script src="~/js/data-table/tableExport.js" asp-append-version="true"></script>
    <script src="~/js/data-table/data-table-active.js" asp-append-version="true"></script>
    <script src="~/js/data-table/bootstrap-table-editable.js" asp-append-version="true"></script>
    <script src="~/js/data-table/bootstrap-editable.js" asp-append-version="true"></script>
    <script src="~/js/data-table/bootstrap-table-resizable.js" asp-append-version="true"></script>
    <script src="~/js/data-table/colResizable-1.5.source.js" asp-append-version="true"></script>
    <script src="~/js/data-table/bootstrap-table-export.js" asp-append-version="true"></script>
    <!--  editable JS
        ============================================ -->
    <script src="~/js/editable/jquery.mockjax.js" asp-append-version="true"></script>
    <script src="~/js/editable/mock-active.js" asp-append-version="true"></script>
    <script src="~/js/editable/select2.js" asp-append-version="true"></script>
    <script src="~/js/editable/moment.min.js" asp-append-version="true"></script>
    <script src="~/js/editable/bootstrap-datetimepicker.js" asp-append-version="true"></script>
    <script src="~/js/editable/bootstrap-editable.js" asp-append-version="true"></script>
    <script src="~/js/editable/xediable-active.js" asp-append-version="true"></script>
    <!-- Chart JS
        ============================================ -->
    <script src="~/js/chart/jquery.peity.min.js" asp-append-version="true"></script>
    <script src="~/js/peity/peity-active.js" asp-append-version="true"></script>
    <!-- morrisjs JS
       ============================================ -->
    <script src="~/js/morrisjs/raphael-min.js" asp-append-version="true"></script>
    <script src="~/js/morrisjs/morris.js" asp-append-version="true"></script>
    <script src="~/js/morrisjs/home3-active.js" asp-append-version="true"></script>
    <!-- morrisjs JS
        ============================================ -->
    <script src="~/js/sparkline/jquery.sparkline.min.js" asp-append-version="true"></script>
    <script src="~/js/sparkline/jquery.charts-sparkline.js" asp-append-version="true"></script>
    <script src="~/js/sparkline/sparkline-active.js" asp-append-version="true"></script>
 
    <script src='~/dist/index.global.js'></script>

    <!-- form validate JS
        ============================================ -->
    <script src="~/js/form-validation/jquery.form.min.js"asp-append-version="true"></script>
    <script src="~/js/form-validation/jquery.validate.min.js"asp-append-version="true"></script>
    <script src="~/js/form-validation/form-active.js" asp-append-version="true"></script>
    <!-- summernote JS
        ============================================ -->
    <script src="~/js/summernote/summernote.min.js" asp-append-version="true"></script>
    <script src="~/js/summernote/summernote-active.js" asp-append-version="true"></script>
    <!-- tab JS
        ============================================ -->
    <script src="~/js/tab.js" asp-append-version="true"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/js/plugins.js" asp-append-version="true"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/js/main.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
